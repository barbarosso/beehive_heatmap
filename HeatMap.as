package  {		import flash.display.MovieClip;	import flash.events.Event;	import flash.net.URLRequest;			public class HeatMap extends MovieClip {				private var hive:MovieClip = new MovieClip();		private var box:MovieClip;		private var frame:MovieClip;		private var sensor:Sensor;				private function rotate(e:Event):void {			hive.rotationY = mouseX;		}				public function HeatMap() {			hive.x = stage.stageWidth/2;			hive.y = 400;			hive.z = 300;			addChild(hive);			addEventListener(Event.ENTER_FRAME, rotate);						for (var b:int = 0; b < 3; b++) {				box = new MovieClip();				box.height = 200 * b;				for (var i:int = 0; i < 8; i++) {					frame = new MovieClip();					frame.z = (40 * i) - 140;					trace(frame.z);					for (var x:int = 0; x < 4; x++) {						for (var y:int = 0; y < 4; y++) {							sensor = new Sensor();							sensor.alpha = Math.random();							frame.addChild(sensor);							sensor.x = sensor.width * x;							sensor.y = sensor.width * y;						}										}					frame.x = -frame.width/2;					box.addChild(frame);				}				hive.addChild(box);			}		}			}	}