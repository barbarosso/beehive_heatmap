package  {		import flash.display.MovieClip;	import flash.events.*;	import flash.net.URLRequest;			public class HeatMap extends MovieClip {				private var hive:MovieClip = new MovieClip();		private var box:MovieClip;		private var frame:MovieClip;		private var sensor:Sensor;				private function rotate(e:Event):void {			hive.rotationY = mouseX;		}				private function clicked(e:Event):void {			hive.rotationY = 0;			removeEventListener(Event.ENTER_FRAME, rotate, false);		}				public function HeatMap() {			hive.x = stage.stageWidth/2;			hive.y = stage.stageHeight/2;			//hive.z = 300;			addChild(hive);			addEventListener(Event.ENTER_FRAME, rotate, false);			addEventListener(MouseEvent.CLICK, clicked);						for (var b:int = 0; b < 2; b++) {				box = new MovieClip();				hive.addChild(box);								for (var i:int = 0; i < 8; i++) {					frame = new MovieClip();					frame.z = (40 * i) - 140;					for (var x:int = 0; x < 4; x++) {						for (var y:int = 0; y < 4; y++) {							sensor = new Sensor();							sensor.alpha = Math.random();							frame.addChild(sensor);							sensor.x = sensor.width * x;							sensor.y = sensor.width * y;						}										}					box.x = -frame.width/2 + 20;					box.y = (-frame.height - 10) * b;					box.addChild(frame);				}			}		}			}	}